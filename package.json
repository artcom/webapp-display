{
  "name": "webapp-display",
  "private": true,
  "main": "src/main.js",
  "version": "3.0.0",
  "scripts": {
    "package:win": "electron-builder build --win",
    "dev": "electron .",
    "lint": "eslint src",
    "start": "electron . --fullscreen"
  },
  "build": {
    "appId": "webapp-player",
    "asar": true,
    "directories": {
      "output": "build"
    },
    "files": [
      "src"
    ],
    "win": {
      "target": [
        "dir"
      ]
    }
  },
  "engines": {
    "node": "14.17.0"
  },
  "dependencies": {
    "@artcom/bootstrap-client": "^4.3.0",
    "axios": "^0.21.1",
    "lodash.frompairs": "^4.0.1",
    "lodash.kebabcase": "^4.1.1",
    "lodash.omitby": "^4.6.0",
    "minimist": "^1.2.5"
  },
  "devDependencies": {
    "electron": "^13.1.2",
    "electron-builder": "^22.11.7",
    "eslint": "^7.28.0",
    "eslint-config-artcom": "^0.11.0",
    "eslint-plugin-import": "^2.23.4"
  }
}
