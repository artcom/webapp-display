{
  "name": "webapp-display",
  "private": true,
  "main": "src/main.js",
  "version": "2.3.1",
  "scripts": {
    "package:win": "electron-builder build --win",
    "dev": "electron .",
    "lint": "eslint src",
    "start": "electron . --fullscreen"
  },
  "build": {
    "appId": "webapp-player",
    "asar": true,
    "directories": {
      "output": "build"
    },
    "files": [
      "src"
    ],
    "win": {
      "target": [
        "dir"
      ]
    }
  },
  "engines": {
    "node": "12.11.0"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "delay": "^4.3.0",
    "electron-debug": "^3.0.1",
    "lodash.frompairs": "^4.0.1",
    "lodash.kebabcase": "^4.1.1",
    "lodash.omitby": "^4.6.0",
    "minimist": "^1.2.5",
    "mqtt-topping": "^0.8.12"
  },
  "devDependencies": {
    "devtron": "^1.4.0",
    "electron": "^8.0.0",
    "electron-builder": "^21.2.0",
    "eslint": "^6.8.0",
    "eslint-config-artcom": "^0.10.1",
    "eslint-plugin-import": "^2.20.1"
  }
}
